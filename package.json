{
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "vite": "vite",
    "vite:build": "vite build",
    "dev": "mix",
    "development": "mix",
    "start": "mix watch",
    "start-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "production": "mix --production",
    "translate": "yarn translate:pot && yarn translate:update",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --include=\"app,resources\"",
    "translate:update": "wp i18n update-po ./resources/lang/sage.pot ./resources/lang/*.po",
    "translate:compile": "yarn translate:mo && yarn translate:js",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print",
    "translate:mo": "wp i18n make-mo ./resources/lang ./resources/lang"
  },
  "devDependencies": {
    "@tinypixelco/laravel-mix-wp-blocks": "^1.0.0",
    "@wordpress/babel-preset-default": "^4.12.1",
    "@wordpress/browserslist-config": "^2.6.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.5.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.27.5",
    "laravel-mix": "^6.0.49",
    "laravel-mix-copy-watched": "^3.0.0",
    "laravel-mix-purgecss": "^6.0.0",
    "npm-run-all": "^4.1",
    "postcss": "^8.4.25",
    "purgecss-with-gutenberg": "^1.0.2",
    "purgecss-with-wordpress": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^20.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@barba/css": "^2.1.15",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@studio-freight/lenis": "^1.0.16",
    "@wordpress/blocks": "^12.14.0",
    "@wordpress/dom-ready": "^3.37.0",
    "@wordpress/edit-post": "^7.14.0",
    "alpinejs": "^3.12.2",
    "bulma": "^0.9.4",
    "bulma-helpers": "^0.3.12",
    "feather-icons": "^4.28.0",
    "gsap": "^3.6.1",
    "jarallax": "^1.12.1",
    "jquery": "^3.7.0",
    "laravel-vite-plugin": "^0.7.8",
    "loconative-scroll": "https://github.com/quentinhocde/loconative-scroll",
    "mouse-follower": "^1.1.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-output-manifest": "^2.0.0",
    "vite": "^4.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@3.6.1"
}
